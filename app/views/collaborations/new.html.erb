<h1>Add User</h1>

<%= form_for [@wiki, Collaboration.new()] do |f| %>

<div class="form-group">
  <%= f.collection_select :user_id, @wiki.available_users, :id, :name, :prompt => true %>
</div>

<div class="form-group">
  <%= f.submit "Save", class: 'green-btn centered-text btn-success' %>
</div>

<% end %>